{
  "name": "resvu-api",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index",
    "start": "npm run build && node dist/index.js",
    "clean": "rm -rf dist",
    "build": "tsc",
    "watch": "tsc -w",
    "schema:generate": "graphql-codegen --config codegen.yml",
    "schema:publish": "rover graph publish resvu-api-ggo96c@current --schema ./src/generated/schema.graphql",
    "db:init": "npx sequelize-cli db:create",
    "migrate:generate": "node ./src/db/migrate create --folder ./src/db/migrations --name",
    "db:migrate": "node ./src/db/migrate up",
    "db:rollback": "node ./src/db/migrate down",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "model:sync": "sequelize-auto -o './src/models' -c './src/db/config/db.config.development.json' -l ts --sg true --skipTables SequelizeMeta",
    "model:generate": "npx sequelize-cli model:generate --name",
    "server:deploy": "graphql-codegen --config codegen.yml && tsc && gcloud app deploy && rover graph publish resvu-api-ggo96c@current --schema ./src/generated/schema.graphql",
    "test": "NODE_ENV=test jest --config jest.config.ts ./**/*.spec.ts"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/ioredis": "^4.28.7",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "graphql": "^16.6.0",
    "graphql-middleware": "^6.1.33",
    "graphql-modules": "^2.1.0",
    "graphql-redis-subscriptions": "^2.4.2",
    "graphql-scalars": "1.14.1",
    "graphql-shield": "^7.6.5",
    "graphql-subscriptions": "^2.0.0",
    "ioredis": "^4.28.5",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "sequelize-auto": "^0.8.8",
    "subscriptions-transport-ws": "^0.11.0",
    "umzug": "^3.2.1"
  },
  "devDependencies": {
    "@apollo/rover": "^0.5.4",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-document-nodes": "2.3.12",
    "@graphql-codegen/typescript-resolvers": "2.7.12",
    "@types/chai-http": "^4.2.0",
    "@types/graphql": "14.5.0",
    "@types/jest": "^29.4.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.11.22",
    "@types/sinon-chai": "^3.2.9",
    "@types/ws": "8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "gql-query-builder": "^3.8.0",
    "jest": "^29.4.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "sequelize-mock": "^0.10.2",
    "sequelize-test-helpers": "^1.4.3",
    "sinon-chai": "^3.7.0",
    "ts-jest": "^29.0.5",
    "ts-mocha": "^10.0.0",
    "ts-node": "10.8.1",
    "ts-node-dev": "1.1.8",
    "ts-sinon": "^2.0.2",
    "typescript": "*"
  }
}
